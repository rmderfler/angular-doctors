<h1>Edit doctor</h1>

<%= render('layouts/errors', :object => @doctor) %>

<form action="/doctors/<%= @doctor.id %>" method="post">
  <input name="_method" type="hidden" value="patch">
  <label for="doctor_name">Name</label>
  <input id="doctor_name" name="doctor[name]" type="text" value="<%= @doctor.name %>">

  <button>Update Doctor</button>
</form>